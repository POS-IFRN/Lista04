<Window x:Class="VisaoWPF.vndVeiculo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisaoWPF"
        mc:Ignorable="d"
        Title="vndVeiculo" Height="300" Width="430.864">
    <Grid>
        <TextBox x:Name="txtId" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="ID" VerticalAlignment="Top" Width="120" IsEnabled="False"/>
        <DatePicker x:Name="dtVenda" HorizontalAlignment="Left" Margin="135,9,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtVenda" HorizontalAlignment="Left" Height="23" Margin="242,10,0,0" TextWrapping="Wrap" Text="Valor Venda" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Vender" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
